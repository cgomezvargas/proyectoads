					<ul class="nav navbar-nav">
						<li><a runat="server" visible="false" id="aUsuarios" href="~/Usuarios/ListaUsuarios.aspx">Usuarios</a></li>
						<li><a runat="server" visible="false" id="aRequisiciones" href="~/Requisicion/ListaRequisiciones.aspx">Requisiciones</a></li>
						<li>
							<asp:LinkButton ID="lbCerrarSesion" OnClick="lbCerrarSesion_Click" runat="server">Cerrar Sesión</asp:LinkButton>
